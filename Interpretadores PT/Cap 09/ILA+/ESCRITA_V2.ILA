// ESCRITA_V2
// Em ILA não é possível definir o tipo "BIMESTRE".
variaveis
  matriz composta ALUNO
    caracter NOME
    caracter TURMA
    numerico SALA
    matriz numerico NOTAS[4]
  fim_composta
  numerico I
inicio
  escrever ALUNO:NOME  
  escrever ALUNO:TURMA  
  escrever ALUNO:SALA 
  para I = 1 ate 4 passo 1 
    escrever ALUNO:NOTAS[I] 
  proximo
fim
