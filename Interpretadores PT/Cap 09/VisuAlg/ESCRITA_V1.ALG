algoritmo "ESCRITA_V1"
tipo
  CAD_ALUNO = registro
                NOME  : caracter
                TURMA : caracter
                SALA  : inteiro
                NOTA1 : real
                NOTA2 : real
                NOTA3 : real
                NOTA4 : real
              fimregistro
var
  ALUNO : cad_aluno
inicio
  escreva(ALUNO.NOME)
  escreva(ALUNO.TURMA)
  escreva(ALUNO.SALA)
  escreva(ALUNO.NOTA1)
  escreva(ALUNO.NOTA2)
  escreva(ALUNO.NOTA3)
  escreva(ALUNO.NOTA4)
fimalgoritmo
