algoritmo // DECISAO_SEQUENCIAL
declare
  N numerico
  leia N
  se (N = 1) entao
    inicio
      escreva "você entrou o valor 1"
    fim
  se (N = 2) entao
    inicio
      escreva "você entrou o valor 2"
    fim
  se (N < 1) entao
    inicio
      escreva "você entrou valor muito baixo"
    fim
  se (N > 2) entao
    inicio
      escreva "você entrou valor muito alto"
    fim
fim_algoritmo.
