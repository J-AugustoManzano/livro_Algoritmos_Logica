// programa DECISAO_SEQUENCIAL
inicio
  variavel inteiro N  
  ler N
  se (N = 1) entao
    escrever "você entrou o valor 1"
  fimse 
  se (N = 2) entao
    escrever "você entrou o valor 2"
  fimse
  se (N < 1) entao
    escrever "você entrou valor muito baixo"
  fimse
  se (N > 2) entao
    escrever "você entrou valor muito alto"
  fimse
fim
