algoritmo MEDIA_GERAL_V3;
variáveis
  I : inteiro;
  MD : matriz[8] de reais;
  SOMA, MEDIA : real;
fim-variáveis
início
  SOMA := 0;
  para I de 1 até 8 passo 1 faça
    MD[I - 1] := leia(); 
    SOMA := SOMA + MD[I - 1];
  fim-para
  MEDIA := SOMA / 8;
  para I de 1 até 8 passo 1 faça
    imprima(MD[I - 1]); 
  fim-para
  imprima(MEDIA);
fim

