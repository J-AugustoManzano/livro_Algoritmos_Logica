programa  // FATORIAL_EX5
{
  funcao inicio()
  {
    inteiro CONT, FAT 
    FAT = 1
    CONT = 1
    enquanto (verdadeiro)          
    {
      FAT = FAT * CONT
      se (CONT > 4)
      {
        pare	
      }
      CONT = CONT + 1
    }
    escreva("5! = ", FAT)
  }
}
