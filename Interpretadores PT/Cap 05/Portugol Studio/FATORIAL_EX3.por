programa  // FATORIAL_EX3
{
  funcao inicio()
  {
    inteiro CONT, FAT 
    FAT = 1
    CONT = 1
    faca          
    {
      FAT = FAT * CONT
      CONT = CONT + 1
    }
    enquanto (nao (CONT > 5))
    escreva("5! = ", FAT)
  }
}
